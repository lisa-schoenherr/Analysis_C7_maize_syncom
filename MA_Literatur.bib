@article{Garrell2025a,
abstract = {{\textless}p{\textgreater} Microbes are ubiquitous in the rhizosphere and play crucial roles in plant health, yet the metabolisms and physiologies of individual species {\textless}italic{\textgreater}in planta{\textless}/italic{\textgreater} remain poorly understood. In this study, we examined microbial gene expression in response to the maize root environment for seven bacterial species originally isolated from maize roots. We grew each species individually, both {\textless}italic{\textgreater}in vitro{\textless}/italic{\textgreater} in a minimal medium and {\textless}italic{\textgreater}in planta{\textless}/italic{\textgreater} , and used differential metaproteomics to identify functions upregulated specifically when bacteria are grown on maize roots. We identified between 1,500 and 2,100 proteins from each species, with approximately 30-70{\%} of these proteins being differentially abundant between the two conditions. While we found that transporter proteins were upregulated in all species {\textless}italic{\textgreater}in planta{\textless}/italic{\textgreater} , all other differentially abundant functions varied greatly between species, suggesting niche specialization in root-associated microbes. Indeed, {\textless}italic{\textgreater}in vitro{\textless}/italic{\textgreater} assays confirmed that {\textless}italic{\textgreater}Curtobacterium pusillum{\textless}/italic{\textgreater} likely degrades plant hemicellulose, {\textless}italic{\textgreater}Enterobacter ludwigii{\textless}/italic{\textgreater} may benefit the plant by phosphate solubilization, and {\textless}italic{\textgreater}Herbaspirillum robiniae{\textless}/italic{\textgreater} colonizes maize roots more effectively when both of its Type VI Secretion Systems are functional. Together, our findings highlight both conserved and species-specific bacterial strategies for growth in the root environment and lay a foundation for future work investigating the mechanisms underlying plant-microbiota interactions. {\textless}/p{\textgreater}},
annote = {Ashley Beck Paper},
author = {Garrell, Anna-Katharina and Cheadle, John and Crook, Nathan and Pal, Gaurav and Septer, Alecia N. and Wagner, Maggie R. and Beck, Ashley E. and Kleiner, Manuel},
doi = {10.1101/2025.06.02.657423},
file = {:home/lisa/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Garrell et al. - 2025 - Differential metaproteomics of bacteria grown iin vitroi and iin plantai reveals functions used during growth on.pdf:pdf},
institution = {bioRxiv},
month = {jun},
title = {{Differential metaproteomics of bacteria grown {\textless}i{\textgreater}in vitro{\textless}/i{\textgreater} and {\textless}i{\textgreater}in planta{\textless}/i{\textgreater} reveals functions used during growth on maize roots}},

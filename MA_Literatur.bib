file = {:home/lisa/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Predl et al. - 2024 - PyCoMo a python package for community metabolic model creation and analysis.pdf:pdf},
